{{#collection tag="header"}}
  <table class="table table-striped">
    <thead>
      <th>Location</th>
      <th>Lattitude</th>
      <th>Longitude</th>
      <th>Time</th>
      <th>Wind Speed</th>
      <th>Wind Bearing</th>
      <th>Temperature</th>
      <th>Summary</th>
    </thead>
    <tbody>
      <tr>
        <td>{{location}}</td>
        <td>{{latitude}}</td>
        <td>{{longitude}}</td>
        <td>{{time}}</td>
        <td>{{windSpeed}}</td>
        <td>{{windBearing}}</td>
        <td>{{temperature}}</td>
        <td>{{summary}}
      </tr>
    </tbody>
  </table>
{{/collection}}
